{
    "name": "Adskipper",
    "description": "A simple extention to skip those annoying youtube ads",
    "version": "1.0",

    "content_scripts":[{
        "js": ["script.js"],
        "run_at": "document_idle",
        "matches":["<all_urls>"]
         }],
    
    "permissions":["tabs","activeTab"],
    "manifest_version": 3
}